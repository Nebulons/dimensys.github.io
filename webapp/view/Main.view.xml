<mvc:View
    controllerName="be.dimensys.assetperformance.assetperformance.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page">
					<content>
						<VBox width="100%" height="100%" alignItems="Center" justifyContent="Center">
							<HBox>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="SAP IAM"
									press="pressIAM" url="https://www.sap.com/">
									<TileContent>
										<ImageContent src="test-resources/sap/m/demokit/sample/GenericTileAsLaunchTile/images/SAPLogoLargeTile_28px_height.png" />
									</TileContent>
								</GenericTile>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Dimensys Accelerators"
									press="pressAcc" url="https://www.sap.com/">
									<TileContent>
										<ImageContent src="data:image/png;base64," />
									</TileContent>
								</GenericTile>
							</HBox>
							<HBox>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="SAP FSM Scheduling"
									press="pressScheduling" url="https://www.sap.com/">
									<TileContent>
										<ImageContent src="https://image.winudf.com/v2/image1/Y29tLnNhcC5mc21faWNvbl8xNjMxNTE5MTgzXzA0MQ/icon.png?w=170&#38;fakeurl=1" />
									</TileContent>
								</GenericTile>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="SAP FSM Mobile execution"
									press="pressMobile" url="https://www.sap.com/">
									<TileContent>
										<ImageContent src="https://image.winudf.com/v2/image1/Y29tLnNhcC5mc21faWNvbl8xNjMxNTE5MTgzXzA0MQ/icon.png?w=170&#38;fakeurl=1" />
									</TileContent>
								</GenericTile>
							</HBox>
						</VBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
